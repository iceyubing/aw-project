{
      "SubmitFeedback": {
        "type": "object",
        "required": [
          "user_id",
          "product_id",
          "rating"
        ],
        "properties": {
          "user_id": {
            "type": "integer",
            "format": "int32",
            "description": "ID of the user submitting the feedback"
          },
          "product_id": {
            "type": "integer",
            "format": "int32",
            "description": "ID of the product being reviewed"
          },
          "rating": {
            "type": "integer",
            "format": "int32",
            "description": "Rating from 1 to 5 stars"
          },
          "comment": {
            "type": "string",
            "description": "Optional comment for the feedback"
          }
        }
      }
}
